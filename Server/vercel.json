{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/Routes/categoryRoutes", "dest": "/api/categoryRoutes.js" },
    { "src": "/Routes/productRoutes", "dest": "/api/productRoutes.js" },
    { "src": "/Routes/userRoutes", "dest": "/api/userRoutes.js" }
  ]
}
