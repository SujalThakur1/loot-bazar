{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/Routes/category", "dest": "/api/categoryRoutes.js" },
    { "src": "/Routes/product", "dest": "/api/productRoutes.js" },
    { "src": "/Routes/user", "dest": "/api/userRoutes.js" }
  ]
}
