{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/category/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/product/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/user/(.*)",
      "dest": "/server.js"
    }
  ]
}
